class GameCharacter:

    def __init__(self, name):
        self._name = name
        self._health = 100
        self._mana = 50
        self._level = 1
        
    @property
    def name(self):
        return self._name
 
    @property
    def health(self):
        return self._health

    @health.setter
    def health(self, value):
        pass
